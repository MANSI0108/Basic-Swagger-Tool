{
    "swagger": "2.0",
    "info": {
        "title": "Bookstore API",
        "version": "1.0.0"
    },
    "paths": {
        "/books": {
            "get": {
                "summary": "Get a list of books",
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "title": {
                                        "type": "string"
                                    },
                                    "author": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}